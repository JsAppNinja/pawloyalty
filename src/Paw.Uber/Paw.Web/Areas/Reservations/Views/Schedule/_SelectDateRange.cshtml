
@{
    Layout = null;

}

<div class="row">
    <div class="col-lg-4">
        <label for="SelectDateRange">Select drop off date</label>
        <div class="input-group mb-3">
            <input aria-describedby="date" aria-label="date" class="form-control" id="StartDate" placeholder="DD/MM/YY" type="text">
            <div class="input-group-append">
                <button class="btn btn-outline-blue" type="button"><i class="fas fa-calendar-alt"></i></button>
            </div>
        </div>
    </div>
    <div class="col-lg-8">
        <label for="SelectDateRange">Select dropp off time</label>
        <div class="input-group mb-3 mw200">
            <input aria-label="Choose drop off time" class="form-control" id="StartTime" type="text">
            <div class="input-group-append">
                <span class="input-group-text"><i class="far fa-clock"></i></span>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-4">
        <label for="SelectDateRange">Select pickup date</label>
        <div class="input-group mb-3">
            <input aria-describedby="date" aria-label="date" class="form-control" id="EndDate" placeholder="DD/MM/YY" type="text">
            <div class="input-group-append">
                <button class="btn btn-outline-blue" type="button"><i class="fas fa-calendar-alt"></i></button>
            </div>
        </div>
    </div>
    <div class="col-lg-8">
        <label for="SelectDateRange">Select pickup time</label>
        <div class="input-group mb-3 mw200">
            <input aria-label="Choose drop off time" class="form-control" id="EndTime" type="text">
            <div class="input-group-append">
                <span class="input-group-text"><i class="far fa-clock"></i></span>
            </div>
        </div>
    </div>
</div>